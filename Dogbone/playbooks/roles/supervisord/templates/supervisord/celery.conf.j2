[program:celery]
user={{ run_user }} ;
environment={{ app_env | custom_env_format | join(",") | replace("%", "%%") }}
command={{ virtualenv_path }}/bin/celery -A dogbone worker -l info --time-limit=7200
directory={{ app_location }} ;
stdout_logfile={{ base_location }}logs/celery_stdout.log ;
stderr_logfile={{ base_location }}logs/celery_stderr.log ;
stdout_logfile_maxbytes={{ logfile_maxbytes }} ;
stderr_logfile_maxbytes={{ logfile_maxbytes }} ;
